{
  "name": "definix-frontend",
  "version": "2.2.2",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "versionfile": "echo $npm_package_version > ./build/version.txt",
    "start": "NODE_ENV=development react-scripts start",
    "start:stage": "NODE_ENV=production CI=false dotenv -e .env.staging react-scripts start",
    "start:staging_prev": "NODE_ENV=staging CI=false dotenv -e .env.staging_prev react-scripts start",
    "start:prod": "NODE_ENV=production CI=false dotenv -e .env.production react-scripts start",
    "build": "CI=false react-scripts build",
    "build:development": "NODE_ENV=development CI=false dotenv -e .env.development react-scripts build",
    "build:stage": "NODE_ENV=production CI=false dotenv -e .env.stage react-scripts build",
    "build:production": "NODE_ENV=production CI=false dotenv -e .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lang": "gsheet-to-json ./lang.json ./src/locales",
    "notice": "gsheet-to-json ./notice.json ./src/assets/notice",
    "voting": "gsheet-to-json ./voting.json ./src/assets/voting",
    "generate:config": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/generateConfig"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@crowdin/crowdin-api-client": "^1.10.4",
    "@ethersproject/abi": "^5.0.7",
    "@ethersproject/experimental": "^5.0.1",
    "@fingerlabs/definixswap-uikit-v2": "0.16.25",
    "@material-ui/core": "^4.12.1",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.5.0",
    "@sixnetwork/caverjs-react-core": "^1.1.3",
    "@sixnetwork/klaytn-use-wallet": "1.1.10",
    "@sixnetwork/klip-connector": "1.0.10",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.7",
    "array-sort": "^1.0.0",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.0",
    "canvas-confetti": "^1.3.3",
    "caver-js": "1.6.3-rc.4",
    "chart.js": "^2.9.4",
    "date-fns": "^2.17.0",
    "dayjs": "^1.10.7",
    "definixswap-sdk": "1.0.0",
    "easymde": "^2.15.0",
    "i18next": "^21.4.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "jest-styled-components": "^7.0.5",
    "klaytn-use-wallet": "^1.0.1",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "numeral": "2.0.4",
    "polished": "^4.1.3",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-countup": "^4.3.3",
    "react-datepicker": "^4.5.0",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.8",
    "react-ga": "^3.3.0",
    "react-gtm-module": "^2.0.11",
    "react-helmet-async": "^1.2.2",
    "react-hook-form": "^7.20.2",
    "react-i18next": "^11.13.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-lottie": "^1.2.3",
    "react-magic-slider-dots": "^1.2.2",
    "react-markdown": "^6.0.2",
    "react-paginate": "^8.1.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-slick": "^0.28.1",
    "react-twitter-widgets": "^1.10.0",
    "remark-gfm": "^1.0.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.1.1",
    "typescript": "^4.1.5",
    "use-async-resource": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@pancakeswap-libs/eslint-config-pancake": "1.0.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.19",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.20.0",
    "eslint-plugin-unused-imports": "^1.1.2",
    "husky": "4.3.5",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^9.1.1"
  }
}
