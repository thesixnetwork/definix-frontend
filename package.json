{
  "name": "definix-frontend",
  "version": "2.5.8",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "NODE_ENV=development react-scripts start",
    "start:stage": "NODE_ENV=production CI=false dotenv -e .env.staging react-scripts start",
    "start:production": "NODE_ENV=production CI=false dotenv -e .env.production react-scripts start",
    "build": "CI=false react-scripts build",
    "build:development": "NODE_ENV=development CI=false dotenv -e .env.development react-scripts build",
    "build:stage": "NODE_ENV=production CI=false dotenv -e .env.stage react-scripts build",
    "build:production": "GENERATE_SOURCEMAP=false NODE_ENV=production CI=false dotenv -e .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lang": "gsheet-to-json ./lang.json ./src/locales",
    "notice": "gsheet-to-json ./notice.json ./public",
    "voting": "gsheet-to-json ./voting.json ./src/assets/voting"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
